{
    "manifest_version": 3,
    "name": "MG Web Admin",
    "version": "1.0",
    "description": "A plugin for Mediagistic web designers.",
    "icons": {
      "48": "icon48.png"
    },
    "action": {
      "default_icon": "icon48.png",
      "default_popup": "popup.html",
      "default_title": "MG Web Admin"
    },
    "permissions": [
      "storage"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    }
  }
  